<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.xsp.business01">

    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:supportsRtl="true">
        <activity android:name=".activity.Business01Activity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>

                <data android:scheme="activity"/>
                <data android:host="business01"/>
                <data android:path="/business01"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.Business01SubActivity"/>

    </application>

</manifest>
